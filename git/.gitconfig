# This is Git's per-user configuration file.
[branch]
	sort = -committerdate
[column]
	ui = auto
[commit]
	verbose = true
[core]
	pager = "less --tabs=4 -RFX"
[diff]
	algorithm = histogram
	colorMoved = dimmed-zebra
	mnemonicPrefix = true
	renames = true
[fetch]
	all = true
	prune = true
	pruneTags = true
[help]
	autocorrect = prompt
[init]
	defaultBranch = main
[merge]
	conflictstyle = diff3
[pull]
	rebase = true
[push]
	autoSetupRemote = true
	default = simple
[rebase]
	autoSquash = true
	updateRefs = true
[rerere]
	autoupdate = true
	enabled = true
[tag]
	sort = version:refname
